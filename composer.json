{
  "name": "vaened/dictionary-flow",
  "description": "A library for comprehensive evaluations within a key-value data dictionary, enabling precise condition definitions and data-driven decision-making.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "data-dictionary",
    "evaluation",
    "key-value",
    "data-analysis",
    "decision-making",
    "parser"
  ],
  "autoload": {
    "psr-4": {
      "Vaened\\DictionaryFlow\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vaened\\DictionaryFlow\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Enea Dhack",
      "email": "enea.so@live.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "vaened/support": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3",
    "symfony/var-dumper": "^6.2"
  }
}
